 <querysource>
  <name>dati_sottordine</name>
  <sql>select
 OPPRORP as &quot;opprorp&quot;,
 OPPRORD as &quot;opprord&quot;,
 OPSTORD as &quot;opstord&quot;
from ord_prod
 where 
 OPPRORP=&lt;? value('sottordine') ?>;</sql>
 </querysource>
 <querysource>
  <name>pl_sottordine</name>
  <sql>select 
rpprrig as &quot;rpprrig&quot;,
rpcdpro as &quot;rpcdpro&quot;,
prdstit as &quot;prdstit&quot;,
rpqtric as &quot;rpqtric&quot;,
uccdudc as &quot;uccdudc&quot;,
uccddep||'.'||ubicazione(uccdubi) as &quot;uccdubi&quot;
from rig_prod, catalogo, udc
where rpprorp=&lt;? value('sottordine') ?>
and uccdudc=rpcdudc
and rpcdpro=prcdpro
order by uccdubi;
;</sql>
 </querysource>
