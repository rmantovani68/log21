<report>
 <title>Packing List Ordine Storico</title>
 <name>Packing List Ordine Storico</name>
 <description></description>
 <parameter name="bolla" >Codice Spedizione</parameter>
 <size>A4</size>
 <portrait/>
 <topmargin>10</topmargin>
 <bottommargin>10</bottommargin>
 <rightmargin>20</rightmargin>
 <leftmargin>20</leftmargin>
 <querysource>
  <name>lista_articoli_in_collo</name>
  <sql>select
 r.RPNMCOL as &quot;collo&quot;,
 r.RPCDPRO as &quot;codice&quot;,
 r.RPQTSPE as &quot;quantita&quot;,
 c.PRDSTIT as &quot;titolo&quot;,
 collo_storico_cpnmcpe(r.ordprog,r.rpnmcol) as &quot;qt_collo&quot;,
 collo_storico_cppspre(r.ordprog,r.rpnmcol) as &quot;peso_collo&quot;,
 collo_storico_cptpfor(r.ordprog,r.rpnmcol) as &quot;imballo_collo&quot;
from ric_ord_stor o,rig_prod_stor r,catalogo_stor c where
 o.ORDPROG=&lt;? value('bolla') ?> and
 r.ORDPROG=&lt;? value('bolla') ?> and
 r.RPCDPRO=c.PRCDPRO and 
 o.ronmced=c.prnmced
order by collo, codice;</sql>
 </querysource>
 <querysource>
  <name>dati_ordine</name>
  <sql>select
 ORDPROG as &quot;ordprog&quot;,
 RONMCLL as &quot;colli&quot;,
 RONMCPP as &quot;copie&quot;,
 ropspre/1000 as &quot;peso&quot;,
 data_ora(ROTMRCZ) as &quot;data_lavorazione&quot;,
 RODSCLI1 as &quot;desc_cliente&quot;,
 ROINCLI as &quot;indirizzo_cliente&quot;,
 ROLOCLI as &quot;localita_cliente&quot;,
 ROPRCLI as &quot;provincia_cliente&quot;,
 ROCPCLI as &quot;cap_cliente&quot;,
 ROCDLIN as &quot;linea_spedizione&quot;,
 data_ora('now') as &quot;data_ora&quot;
from ric_ord_stor
 where 
 ORDPROG=&lt;? value('bolla') ?>;</sql>
 </querysource>
 <querysource>
  <name>lista_articoli_per_codice</name>
  <sql>select
 r.RPNMCOL as &quot;collo&quot;,
 r.RPCDPRO as &quot;codice&quot;,
 r.RPQTSPE as &quot;quantita&quot;,
 c.PRDSTIT as &quot;titolo&quot;, copie_articolo_in_ordine_storico(r.ordprog,r.rpcdpro) as &quot;qt_codice&quot;,
 collo_storico_cptpfor(r.ordprog,r.rpnmcol) as &quot;imballo_collo&quot;
from ric_ord_stor o,rig_prod_stor r,catalogo_stor c where
 o.ORDPROG=&lt;? value('bolla') ?> and
 r.ORDPROG=&lt;? value('bolla') ?> and
 o.RPNMCED=c.PRNMCED and
 r.RPCDPRO=c.PRCDPRO
order by qt_codice desc,quantita desc,codice,collo;</sql>
 </querysource>
