<report>
 <title>Packing List Collo Scartato</title>
 <name>Packing List Collo Scartato</name>
 <description></description>
 <parameter name="bolla" >Codice Bolla di Carico</parameter>
 <parameter name="collo" ></parameter>
 <size>A4</size>
 <portrait/>
 <topmargin>10</topmargin>
 <bottommargin>10</bottommargin>
 <rightmargin>20</rightmargin>
 <leftmargin>20</leftmargin>
 <querysource>
  <name>lista_articoli_in_collo</name>
  <sql>select
 r.RPNMCOL as &quot;collo&quot;,
 r.RPCDPRO as &quot;codice&quot;,
 r.RPQTSPE as &quot;quantita&quot;,
 c.PRDSTIT as &quot;titolo&quot;,
 ubicazione_ridotta(r.rpcdubi) as &quot;ubicazione&quot;,
 prodotto_prpspre(r.rpcdpro) as &quot;peso&quot;,
 riga_rppspre(r.ordprog,r.rpnmcol,r.rpcdpro) as &quot;peso_riga&quot;
from rig_prod r,catalogo c where
 r.ORDPROG=&lt;? value('bolla') ?> and 
 r.RPNMCOL=&lt;? value('collo') ?> and
 r.RPCDPRO=c.PRCDPRO
order by codice;</sql>
 </querysource>
 <querysource>
  <name>dati_ordine</name>
  <sql>select
 ORDPROG as &quot;ordprog&quot;,
 ROSTATO as &quot;stato_ordine&quot;,
 RONMCED as &quot;cedola&quot;,
 RODSCLI1 as &quot;desc_cliente&quot;,
 ROINCLI as &quot;indirizzo_cliente&quot;,
 ROLOCLI as &quot;localita_cliente&quot;,
 ROPRCLI as &quot;provincia_cliente&quot;,
 ROCPCLI as &quot;cap_cliente&quot;,
 data_ora('now') as &quot;data_ora&quot;
from ric_ord
 where 
 ORDPROG=&lt;? value('bolla') ?>;</sql>
 </querysource>
 <querysource>
  <name>dati_collo</name>
  <sql>select
 cpnmcol,
 cptpfor,
 cpstato,
 cpnmcol,
 cpnmrgh,
 cpnmcpe,
 cpcntsc,
 collo_cppsrea(ordprog,cpnmcol) as &quot;peso_reale&quot;,
 collo_cppspre(ordprog,cpnmcol) as &quot;peso_previsto&quot;
from col_prod where ordprog=&lt;? value('bolla') ?> and cpnmcol=&lt;? value('collo') ?>;</sql>
 </querysource>
