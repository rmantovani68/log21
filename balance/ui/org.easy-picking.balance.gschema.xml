        <key name="tabella-ordini-lista-ordini"     type="s"> <default>'select r.rostato as "S", r.roswcol as "BANC", r.roprgln as "PRG", r.ronmced as "CED", data(rotmrcz) as "DATA",r.ordprog as "ORDINE", r.rocdrid as "COD.CLI", r.rodscli as "RAGIONE SOCIALE", r.rocdlin as "LN", r.ronmcll as "COLLI", r.ronmrgh as "RIGHE", r.ronmcpe as "COPIE", r.ropspre::float4/1000 as "PESO PRE(Kg)", r.ropsrea::float4/1000 as "PESO REA(Kg)", r.rolocli as "LOC.", r.rocpcli as "CAP", r.roprcli as "PRV",r.rodsnot as "Nota Corriere"  from ric_ord as r where r.ordtipo=\'%ORDTIPO%\' order by r.ordprog;'</default>          <summary>tabella-ordini-lista-ordini</summary> <description>tabella-ordini-lista-ordini</description> </key>
        <key name="tabella-ordini-lista-ordini-key" type="i"> <default>5</default>          <summary>tabella-ordini-lista-ordini-key</summary> <description>tabella-ordini-lista-ordini-key</description> </key>

        <key name="statistica-impianto"             type="s"> <default>'select roswcol as "P/C",rostato as "STATO",count(rostato) as "ORDINI",sum(ronmcll) as "COLLI",sum(ronmrgh) as "RIGHE",sum(ronmcpe) as "COPIE" from ric_ord where ordtipo=\'%ORDTIPO%\' group by roswcol,rostato;'</default>          <summary>statistica-impianto</summary> <description>statistica-impianto</description> </key>
        <key name="lista-distribuzione"             type="s"> <default>'select sdnmced as "CEDOLA",sdcdlin as "LINEA", sddslin as "DESCRIZIONE LINEA", sdnmort as "ORDINI",sdnmclt as "COLLI",sdnmrgh as "RIGHE",sdnmcpe as "COPIE",sdpspre::int4/1000 as "PESO ",sdnmori as "RAC",sdnmcli as "COLLI",sdnmore as "EVASI",sdnmcle as "COLLI",sdnmorx as "XAB",sdnmclx as "COLLI",sdnmord as "DIST",sdnmcld as "COLLI",sdnmors as "HOST",sdnmcls as "COLLI" from sel_dist_tmp_delta_%ORDTIPO% order by sdnmced,sdcdlin;'</default>          <summary>lista-distribuzione</summary> <description>lista-distribuzione</description> </key>

        <key name="lista-calcolo-imballi-periodo"         type="s">  
        <default>'
select 
    c.cptpfor as "FORMATO",
    count(cptpfor) as "N. COLLI" from ric_ord o, col_prod c 
    where o.ordprog=c.ordprog and  %KEY% group by cptpfor 
union select 
    c.cptpfor as "FORMATO",
    count(cptpfor) as "N. COLLI" from ric_ord_stor o, col_prod_stor c 
    where o.ordprog=c.ordprog and  %KEY% group by cptpfor 
order by 1 ;
        '</default> <summary>lista-imballi</summary> <description>lista-imballi</description> </key>


        <key name="lista-calcolo-preconfezionati-periodo" type="s"> 
        <default>'
select 
    p.prfcpcf as "FORMATO",
    count(prfcpcf) as "N. COLLI" from ric_ord o,
    col_prod c, rig_prod r, catalogo p 
    where o.ordprog=c.ordprog and cptpfor=\'C\' and  %KEY% and c.ordprog=r.ordprog and c.cpnmcol=r.rpnmcol and r.rpcdpro=p.prcdpro group by prfcpcf  
union select 
    p.prfcpcf as "FORMATO",
    count(prfcpcf) as "N. COLLI" from ric_ord_stor o,
    col_prod_stor c, rig_prod_stor r, catalogo p 
    where o.ordprog=c.ordprog and cptpfor=\'C\' and  %KEY% and c.ordprog=r.ordprog and c.cpnmcol=r.rpnmcol and r.rpcdpro=p.prcdpro group by prfcpcf 
order by 1 ;
        '</default> <summary>lista-preconfezionati</summary> <description>lista-preconfezionati</description> </key>


