echo ftp_send_log_sequence   ; pg_dump -h database mondadori -O -s -t ftp_send_log_sequence      > ftp_send_log_sequence.dump.sql
echo ftp_send_log            ; pg_dump -h database mondadori -O -s -t ftp_send_log               > ftp_send_log.dump.sql
echo ftp_sender_log_sequence ; pg_dump -h database mondadori -O -s -t ftp_sender_log_sequence    > ftp_sender_log_sequence.dump.sql
echo ftp_sender_log          ; pg_dump -h database mondadori -O -s -t ftp_sender_log             > ftp_sender_log.dump.sql
